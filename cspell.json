{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en-GB",
    "languageId": "python",
    "useGitignore": true,
    "dictionaries": [
        "!aws",
        "!companies",
        "!cryptocurrencies",
        "!web-services"
    ],
    "cache": {
        "cacheFormat": "universal",
        "cacheLocation": ".cache/.cspellcache",
        "cacheStrategy": "content",
        "useCache": true
    },
    "ignorePaths": [
        "cspell.json",
        "mkdocs.yml",
        "pyproject.toml",
        "CHANGELOG.md"
    ],
    "words": [
        "datasheet",
        "datasheets",
        "deserialised",
        "dtype",
        "etree",
        "loadtxt",
        "loggable",
        "Measurand",
        "memmap",
        "ndarray",
        "numpy",
        "pycon",
        "recarray",
        "recfunctions",
        "savetxt",
        "setflags",
        "ureal"
    ]
}

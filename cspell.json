{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en-GB",
    "languageId": "python",
    "useGitignore": true,
    "dictionaries": [
        "!aws",
        "!companies",
        "!cryptocurrencies",
        "!web-services"
    ],
    "cache": {
        "cacheFormat": "universal",
        "cacheLocation": ".cache/.cspellcache",
        "cacheStrategy": "content",
        "useCache": true
    },
    "ignorePaths": [
        "cspell.json",
        "mkdocs.yml",
        "pyproject.toml",
        "CHANGELOG.md",
        "tests/resources"
    ],
    "words": [
        "allclose",
        "ASRL",
        "ASRLCOM",
        "Borbely",
        "Callendar",
        "cbrt",
        "datasheet",
        "datasheets",
        "deserialised",
        "DGRAM",
        "dtype",
        "Dusen",
        "EIGHTBITS",
        "etree",
        "FIVEBITS",
        "frombuffer",
        "fromiter",
        "gethostname",
        "GPIB",
        "gtrsim",
        "HISLIP",
        "infty",
        "INSTR",
        "itemsize",
        "loadtxt",
        "loggable",
        "Measurand",
        "memmap",
        "MSLE",
        "MSLT",
        "nbytes",
        "ndarray",
        "NIDAQ",
        "numpy",
        "Prologix",
        "pycon",
        "recarray",
        "recfunctions",
        "rtol",
        "savetxt",
        "SCPI",
        "setflags",
        "SEVENBITS",
        "SIXBITS",
        "STOPBITS",
        "TCPIP",
        "tobytes",
        "ureal"
    ]
}
